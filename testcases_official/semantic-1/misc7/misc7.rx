// Binary search algorithm
// Search for a target value in a sorted array
fn binary_search(arr: &[i32], target: i32, size: i32) -> i32 {
    let mut left = 0;
    let mut right = size - 1;
    
    while (left <= right) {
        let mid = left + (right - left) / 2;
        
        if (arr[mid as usize] == target) {
            return mid;
        }
        
        if (arr[mid as usize] < target) {
            left = mid + 1;
        } else {
            right = mid - 1;
        }
    }
    
    return -1; // Not found
}

fn main() {
    let n = readInt();
    let target = readInt();
    
    let mut arr: [i32; 20] = [0; 20];
    let mut i = 0;
    
    // Read sorted array
    while (i < n) {
        arr[i as usize] = readInt();
        i += 1;
    }
    
    let result = binary_search(&arr, target, n);
    printInt(result);
    exit(0);
}
