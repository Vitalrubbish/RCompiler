fn main() {
    let mut fib = [0u32; 10];
    fib[1] = 1;
    let mut i = 2usize;
    while (i < 10) {
        fib[i] = fib[i - 1] + fib[i - 2];
        i += 1;
    }
    let _ = fib;
    exit(0);
}