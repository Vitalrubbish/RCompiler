fn add(x : i32, y : i32) -> i32 {
    let ans : i32 = x + y;
    return ans;
}

fn minus(x : i32, y : i32) -> i32 {
    let ans : i32 = x - y;
    return ans;
}

fn main() {
    let mut a : i32 = 1;
    let mut b : i32 = 1;
    let mut c : i32 = 0;
    let mut d : i32 = 0;
    c = add(a, b);
    d = minus(a, b);
    exit(0);
}