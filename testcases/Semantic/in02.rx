fn make_origin() -> Point {
    Point { x: 0, y: 0 }
}

// 然后定义 `Point` 结构体
struct Point {
    x: i32,
    y: i32,
}

fn main() {
    let origin = make_origin();
    println("Origin: ({}, {})");
    printlnInt(origin.x);
    printlnInt(origin.y);
    exit(0);
}

