struct Player {
    score: i32,
}

impl Player {
    fn new() -> Player {
        Player { score: 0 }
    }
}

impl Player {
    fn get_score(&self) -> i32 {
        self.score
    }

    fn add_score(&mut self, points: i32) {
        self.score += points;
    }
}

fn main() {
    exit(0);
} // InherentImpl Test: Multiple Implement for 1 struct type.
